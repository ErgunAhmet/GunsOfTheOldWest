@{
    ViewData["Title"] = "Summary";
}

@model Shooter
<div class="container">
    <h3>First name: @Model.FirstName</h3>
    <h3>Last name: @Model.LastName</h3>
    <h3>Email: @Model.Email</h3>
    <h3>Phone number: @Model.PhoneNumber</h3>
    <h3>Game played at: @DateTime.Now.ToString("dd/MM/yyyy")</h3>
    <form asp-controller="Home" asp-action="Index">
        <button id="buttonGoToStart" type="submit">Go To Start</button>
    </form>
</div>
